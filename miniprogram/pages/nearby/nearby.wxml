<!--pages/nearby/nearby.wxml-->
<view class="page-body">
  <view class="page-section page-section-gap">
    <map
      id="myMap"
      style="width: 100%; height: 630px;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      covers="{{covers}}"
      show-location
      show-scale
    >
    <cover-image src="/icon/map_location.png" bindtap="moveToLocation"></cover-image>
    </map>
  </view>
</view>
